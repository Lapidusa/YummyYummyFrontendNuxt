{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@fonts/*": ["./assets/fonts/*"],
      "@assets/*": ["./assets/*"],
      "@icons/*": ["./assets/icons/*"],
      "@styles/*": ["./assets/styles/*"],
      "@components/*": ["./components/*"],
      "@store/*": ["./store/*"],
      "@composable/*": ["./composable/*"],
      "@interfaces/*": ["./interfaces/*"]
    },
    "types": ["@types/node"]
  }
}
